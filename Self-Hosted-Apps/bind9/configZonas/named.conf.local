//Archivo de zona directa roca.home

zone "roca.home" {
    type master;
    file "/etc/bind/db.roca.home";
};

//Archivo de zona inversa 192.168.0.XXX

zone "0.168.192.in-addr.arpa" {
    type master;
    file "/etc/bind/db.192.168.0";
};
